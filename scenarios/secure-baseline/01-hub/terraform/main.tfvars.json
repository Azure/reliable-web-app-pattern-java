{
    "application_name": "${AZURE_ENV_NAME}",
    "environment": "${APP_ENVIRONMENT}",
    "enable_telemetry" : "${ENABLE_TELEMETRY=true}",
    "location": "${AZURE_LOCATION}",
    "jumpbox_vm_size": "${JUMPBOX_VM_SIZE=Standard_B2s}",
    "jumpbox_username": "${JUMPBOX_USERNAME=azureuser}",
    "jumpbox_password": "${JUMPBOX_PASSWORD}",
    "deploy_bastion": "${DEPLOY_BASTION=true}",
    "deploy_jumpbox": "${DEPLOY_JUMPBOX=true}",
    "principal_type": "${PRINCIPAL_TYPE=User}"
}
